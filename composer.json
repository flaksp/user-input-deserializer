{
  "name": "flaksp/user-input-processor",
  "description": "Denormalizer and validator for any kind of user input.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/flaksp/user-input-processor",
  "readme": "./README.md",
  "keywords": [
    "denormalizer",
    "validator",
    "forms"
  ],
  "authors": [
    {
      "name": "Petr Flaks",
      "email": "petr@flaks.dev",
      "homepage": "https://flaks.dev",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/flaksp/user-input-processor/issues",
    "forum": "https://github.com/flaksp/user-input-processor/discussions",
    "source": "https://github.com/flaksp/user-input-processor",
    "docs": "https://github.com/flaksp/user-input-processor/blob/main/README.md"
  },
  "require": {
    "php": ">= 8.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "2.18.6",
    "phpunit/phpunit": "9.5.2",
    "squizlabs/php_codesniffer": "3.5.8"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Flaksp\\UserInputProcessor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Flaksp\\UserInputProcessor\\": "tests/"
    }
  }
}
